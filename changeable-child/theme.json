{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "\"Space Grotesk\", sans-serif",
          "name": "Space Grotesk",
          "slug": "space-grotesk"
        },
        {
          "fontFamily": "system-ui, sans-serif",
          "name": "System Default",
          "slug": "system-ui"
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--space-grotesk)"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--space-grotesk)"
        }
      },
      "button": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--space-grotesk)"
        }
      }
    }
  }
}
